{
  "name": "alva-3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f ./config/.env.development next dev",
    "next:test": "env-cmd -f ./config/.env.test next dev",
    "next:prod": "env-cmd -f ./config/.env.test next build && env-cmd -f ./config/.env.test next start",
    "build": "next build",
    "build:export": "next build && next export",
    "start": "env-cmd -f ./config/.env.production next start",
    "start:prod": "env-cmd -f ./config/.env.production next build && env-cmd -f ./config/.env.production next start -p 3001",
    "test": "jest --watchAll",
    "lint": "eslint . --fix",
    "cypress": "cypress open",
    "copy:reports": "mkdir coverage .nyc_output || true %% cp cypress-coverage/coverage-final.json coverage/from-cypress.json && jest-coverage/coverage-final.json coverage/from-jest.json",
    "combine:reports": "npx nyc merge coverage && mv coverage.json .nyc_output/out.json",
    "code:coverage:report": "npx nyc report --reporter lcov --reporter text-summary --report-dir coverage",
    "generate:code:coverage:report": "yarn copy:reports && yarn combine:reports && yarn code:coverage:report",
    "coverage": "cd cypress-coverage && cd Lcov-report && index.html"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@hookform/resolvers": "2.6.0",
    "@marker.io/browser": "^0.10.0",
    "@material-ui/core": "4.11.4",
    "@material-ui/data-grid": "4.0.0-alpha.32",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@material-ui/pickers": "3.3.10",
    "@material-ui/styles": "4.11.4",
    "antd": "4.16.6",
    "axios": "0.21.1",
    "classnames": "^2.3.1",
    "date-fns": "2.22.1",
    "date-fns-timezone": "^0.1.4",
    "dom-to-image": "^2.6.0",
    "dotenv": "10.0.0",
    "downloadjs": "^1.4.7",
    "env-cmd": "^10.1.0",
    "form-data": "^4.0.0",
    "highcharts": "9.1.2",
    "highcharts-custom-events": "^3.0.10",
    "highcharts-react-official": "^3.0.0",
    "i18next": "20.3.2",
    "intro.js": "4.1.0",
    "intro.js-react": "0.3.0",
    "js-cookie": "2.2.1",
    "lodash": "4.17.21",
    "next": "11.0.1",
    "next-absolute-url": "^1.2.2",
    "next-compose-plugins": "2.2.1",
    "next-cookies": "2.0.3",
    "next-i18next": "8.5.1",
    "primeflex": "^3.1.0",
    "primeicons": "^4.1.0",
    "primereact": "^7.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-flagkit": "2.0.4",
    "react-highlight-words": "0.17.0",
    "react-hook-form": "7.10.1",
    "react-html-parser": "2.0.2",
    "react-i18next": "11.11.1",
    "react-infinite-scroll-component": "6.1.0",
    "react-loaders": "^3.0.1",
    "react-query": "3.18.1",
    "react-redux": "7.2.4",
    "react-responsive": "8.2.0",
    "react-scroll": "1.8.2",
    "react-toastify": "^7.0.4",
    "react-tooltip": "4.2.21",
    "redux": "4.1.0",
    "redux-devtools-extension": "2.13.9",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "rsuite": "4.10.2",
    "sass": "1.35.1",
    "timezone-support": "2.0.2",
    "tiny-glob": "^0.2.9",
    "use-debounce": "7.0.0",
    "uuid": "^8.3.2",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/runtime": "7.14.6",
    "@svgr/webpack": "5.5.0",
    "eslint": "7.30.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.8",
    "lint-staged": ">=11.0.0",
    "prettier": "2.3.2"
  },
  "optionalDependencies": {
    "jest": "27.0.6",
    "nyc": "^15.1.0",
    "cypress": "^9.1.0",
    "cypress-real-events": "^1.6.0",
    "cypress-localstorage-commands": "^1.6.1",
    "@cypress/code-coverage": "^3.9.11",
    "@testing-library/dom": "8.1.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "babel-jest": "27.0.6",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-preset-rsuite": "4.0.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass)$": "<rootDir>/__mocks__/styleMock.js",
      "next-i18next": "<rootDir>/__mocks__/next-i18next.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "nyc": {
    "report-dir": "cypress-coverage"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
